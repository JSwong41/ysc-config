{
  "key": "zy_suoni",
  "name": "资源-梭尼",
  "type": 1,
  "api": "https://suoniapi.com/api.php/provide/vod/",
  "searchable": 1,
  "quickSearch": 1,
  "filterable": 1,
  "filters": [
    {"n": "地区", "v": "大陆,美国,韩国,日本"},
    {"n": "类型", "v": "动作,喜剧,爱情,科幻,恐怖,动画,纪录片"},
    {"n": "年份", "v": "2025,2024,2023,2022,2021,2020,2019及以前"}
  ],
  "list": {
    "url": "https://suoniapi.com/api.php/provide/vod/?ac=list",
    "method": "GET",
    "params": {
      "limit": 20,
      "page": 1,
      "type": ""
    },
    "parse": {
      "id": "vod_id",
      "title": "vod_name",
      "cover": "vod_pic",
      "desc": "vod_remarks",
      "url": "vod_play_url"
    }
  },
  "search": {
    "url": "https://suoniapi.com/api.php/provide/vod/?ac=detail",
    "method": "GET",
    "params": {
      "wd": ""
    },
    "parse": {
      "id": "vod_id",
      "title": "vod_name",
      "cover": "vod_pic",
      "desc": "vod_remarks",
      "url": "vod_play_url"
    }
  },
  "play": {
    "url": "https://suoniapi.com/api.php/provide/vod/?ac=play",
    "method": "GET",
    "params": {
      "id": ""
    },
    "parse": {
      "url": "url",           // 播放地址字段
      "title": "title",       // 影片标题
      "type": "type",         // 播放类型（如 m3u8 或 mp4）
      "episode": "episode"    // 分集信息
    }
  }
}
